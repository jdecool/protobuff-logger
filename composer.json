{
    "name": "jdecool/protobuff-logger",
    "type": "library",
    "keywords": ["log", "logging", "psr-3", "protobuff", "protocol buffers"],
    "description": "A minimal PSR-3 compatible logger using Protocol Buffers",
    "license": "MIT",
    "authors": [
        {
            "name": "Jérémy DECOOL",
            "email": "contact@jdecool.fr"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "psr/log": "^1.0.0",
        "google/protobuf": "^3.5.0"
    },
    "require-dev": {
        "atoum/atoum": "^3.0",
        "atoum/stubs": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "GPBMetadata\\": "build/GPBMetadata",
            "JDecool\\ProtoLogger\\Model\\": "build/JDecool/ProtoLogger/Model",
            "JDecool\\ProtoLogger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JDecool\\ProtoLogger\\Tests\\Units\\": "tests/units"
        }
    }
}
